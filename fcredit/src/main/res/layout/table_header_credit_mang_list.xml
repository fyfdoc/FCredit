<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    >

    <!--操作-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_opt"
        android:layout_width="50dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_alignBottom="@+id/list_no"
        android:layout_alignTop="@+id/list_no"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--序号-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_no"
        android:layout_width="80dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_toRightOf="@+id/list_opt"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--贷款名称-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_credit_name"
        android:layout_width="100dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_toRightOf="@+id/list_no"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--银行-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_bank"
        android:layout_width="80dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_alignBottom="@+id/list_credit_name"
        android:layout_alignTop="@+id/list_credit_name"
        android:layout_toRightOf="@+id/list_credit_name"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--还款日-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_repayment_date"
        android:layout_width="80dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_alignBottom="@+id/list_bank"
        android:layout_alignTop="@+id/list_bank"
        android:layout_toRightOf="@+id/list_bank"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--每月应还-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_repay_month"
        android:layout_width="80dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_alignBottom="@+id/list_repayment_date"
        android:layout_alignTop="@+id/list_repayment_date"
        android:layout_toRightOf="@+id/list_repayment_date"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--利率-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_interest_rate"
        android:layout_width="50dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_alignBottom="@+id/list_repay_month"
        android:layout_alignTop="@+id/list_repay_month"
        android:layout_toRightOf="@+id/list_repay_month"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--贷款总额-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_credit_limit"
        android:layout_width="80dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_alignBottom="@+id/list_interest_rate"
        android:layout_alignTop="@+id/list_interest_rate"
        android:layout_toRightOf="@+id/list_interest_rate"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--贷款期限(月)-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_credit_length"
        android:layout_width="90dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_alignBottom="@+id/list_credit_limit"
        android:layout_alignTop="@+id/list_credit_limit"
        android:layout_toRightOf="@+id/list_credit_limit"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--提款日期-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_draw_date"
        android:layout_width="80dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_alignBottom="@+id/list_credit_length"
        android:layout_alignTop="@+id/list_credit_length"
        android:layout_toRightOf="@+id/list_credit_length"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--本期截止日-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_current_end_date"
        android:layout_width="80dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_alignBottom="@+id/list_draw_date"
        android:layout_alignTop="@+id/list_draw_date"
        android:layout_toRightOf="@+id/list_draw_date"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--贷款有效期(开始)-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_credit_length_start_date"
        android:layout_width="110dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_alignBottom="@+id/list_current_end_date"
        android:layout_alignTop="@+id/list_current_end_date"
        android:layout_toRightOf="@+id/list_current_end_date"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--贷款有效期(结束)-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_credit_length_end_date"
        android:layout_width="110dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_alignBottom="@+id/list_credit_length_start_date"
        android:layout_alignTop="@+id/list_credit_length_start_date"
        android:layout_toRightOf="@+id/list_credit_length_start_date"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--备注-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_comment"
        android:layout_width="150dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_alignBottom="@+id/list_credit_length_end_date"
        android:layout_alignTop="@+id/list_credit_length_end_date"
        android:layout_toRightOf="@+id/list_credit_length_end_date"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

    <!--id hidden-->
    <com.fcredit.widget.table.TableCellTextView
        android:id="@+id/list_id"
        android:layout_width="0dp"
        android:layout_height="@dimen/table_line_height"
        android:layout_alignBottom="@+id/list_comment"
        android:layout_alignTop="@+id/list_comment"
        android:layout_toRightOf="@+id/list_comment"
        android:gravity="center"
        android:textColor="#000"
        android:textSize="13sp" />

</RelativeLayout>